{
  "rules": {
    "curly": true,
    "label-position": true,
    "no-arg": true,
    "class-name": true,
    "prefer-for-of": false,
    "comment-format": [true],
    "interface-name": [true, "always-prefix"],
    "indent": [true, "spaces"],
    "import-spacing": true,
    "new-parens": true,
    "no-unnecessary-initializer": true,
    "no-angle-bracket-type-assertion": false,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-var-requires": true,
    "no-internal-module": true,
    "no-trailing-whitespace": false,
    "no-var-keyword": true,
    "no-invalid-this": true,
    "no-misused-new": true,
    "no-unused-variable": [true],
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-unused-new": true,
    "one-line": [true,
      "check-whitespace",
      "check-catch",
      "check-finally",
      "check-else"
    ],
    "quotemark": [false],
    "semicolon": false,
    "member-ordering": [true, {
      "order": [
        "static-field",
        "static-method",
        "constructor",
        "instance-method"
      ]
    }],
    "triple-equals": [true, "allow-null-check"],
    "typeof-compare": true,
    "typedef": [true,
      "call-signature",
      "arrow-call",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "member-variable-declarations"
    ],
    "typedef-whitespace": [true, {
      "call-signature": "nospace",
      "index-signature": "nospace",
      "parameter": "nospace",
      "property-declaration": "nospace",
      "variable-declaration": "nospace"
    }],
    "variable-name": [true, "ban-keywords"],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-module",
      "check-operator",
      "check-separator",
      "check-type",
      "check-preblock"
    ]
  }
}